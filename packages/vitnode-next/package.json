{
  "name": "vitnode-next",
  "version": "1.0.0",
  "devDependencies": {
    "@hono/swagger-ui": "^0.5.0",
    "@hono/zod-openapi": "^0.18.3",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/node": "^22.10.5",
    "concurrently": "^9.1.2",
    "hono": "^4.6.16",
    "typescript": "^5.7.3"
  },
  "exports": {
    "./*": {
      "import": "./dist/src/*.js",
      "types": "./dist/src/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && swc src -d dist --config-file .swcrc",
    "dev": "concurrently \"tsc -w --preserveWatchOutput\" \"swc src -d dist --config-file .swcrc -w\""
  },
  "type": "module"
}
