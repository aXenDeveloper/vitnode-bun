{
  "name": "vitnode",
  "version": "0.2.0",
  "devDependencies": {
    "@hono/swagger-ui": "^0.5.0",
    "@hono/zod-openapi": "^0.18.3",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "hono": "^4.6.17",
    "next": "^15.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3"
  },
  "bin": {
    "vitnode": "./dist/scripts/index.js"
  },
  "exports": {
    "./*": {
      "import": "./dist/src/*.js",
      "types": "./dist/src/*.d.ts"
    }
  },
  "scripts": {
    "build:scripts": "swc scripts -d dist --config-file .swcrc",
    "build": "tsc && swc src -d dist --config-file .swcrc",
    "dev": "concurrently \"tsc -w --preserveWatchOutput\" \"swc src -d dist --config-file .swcrc -w\""
  },
  "type": "module",
  "dependencies": {
    "next-themes": "^0.4.4"
  }
}
